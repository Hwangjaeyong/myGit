### BASIC ###

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /home/apis/public_html
        ServerName apis.thebom.pro
        SetEnvIfNoCase Request_URI (gif|png|jpg|css|js|bmp|jpeg|swf)$ IMAGE=1
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/apis.thebom.pro-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/apis.thebom.pro-error_log.%Y%m%d"

        <Files ~ ".bak$">
          Order allow,deny
          Deny from all
        </Files>

        <Files ~ ".old$">
          Order allow,deny
          Deny from all
        </Files>
</VirtualHost>

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /home/webview/public_html
        ServerName wv.thebom.pro
        SetEnvIfNoCase Request_URI (gif|png|jpg|css|js|bmp|jpeg|swf)$ IMAGE=1
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/wv.thebom.pro-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/wv.thebom.pro-error_log.%Y%m%d"

        <Files ~ ".bak$">
          Order allow,deny
          Deny from all
        </Files>

        <Files ~ ".old$">
          Order allow,deny
          Deny from all
        </Files>

	<IfModule mod_headers.c>
   	 Header set Access-Control-Allow-Origin "*"
	</IfModule>
</VirtualHost>

