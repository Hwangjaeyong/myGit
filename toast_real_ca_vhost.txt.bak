        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/www.editalk.org-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/www.editalk.org-error_log.%Y%m%d"

