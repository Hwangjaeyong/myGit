### AOKR SA ###

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /home/nyadmin/public_html
        ServerName aokr-sa.thebom.pro
        SetEnvIfNoCase Request_URI (gif|png|jpg|css|js|bmp|jpeg|swf)$ IMAGE=1
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/aokr-sa.thebom.pro-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/aokr-sa.thebom.pro-error_log.%Y%m%d"

        <Files ~ ".bak$">
          Order allow,deny
          Deny from all
        </Files>

        <Files ~ ".old$">
          Order allow,deny
          Deny from all
        </Files>
</VirtualHost>

