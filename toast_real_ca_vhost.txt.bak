### EDITALK - PC ###

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /home/editalk/public_html
        ServerName editalk.org
        ServerAlias www.editalk.org
#        Redirect permanent / https://www.editalk.org

	RewriteEngine On
	RewriteRule  ^(.*)$ https://%{HTTP_HOST}$1     [R=301,L]

