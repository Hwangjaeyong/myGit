### BASIC ###

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /home/www/public_html
        ServerName thebom.pro
        ServerAlias theboms.pro www.thebom.pro
        # Redirect permanent / https://www.thebom.pro
        SetEnvIfNoCase Request_URI (gif|png|jpg|css|js|bmp|jpeg|swf)$ IMAGE=1
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/www.thebom.pro-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/www.thebom.pro-error_log.%Y%m%d"

        <Directory /home/www/public_html/>
         Options FollowSymLinks MultiViews
         AllowOverride All
         Require all granted
        </Directory>

        <Files ~ ".bak$">
         Order allow,deny
         Deny from all
        </Files>

        <Files ~ ".old$">
         Order allow,deny
         Deny from all
        </Files>
</VirtualHost>

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /data/files
        ServerName file.thebom.pro
        Redirect permanent / https://file.thebom.pro
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/file.thebom.pro-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/file.thebom.pro-error_log.%Y%m%d"

        <Directory /data/files/>
         Options FollowSymLinks MultiViews
         AllowOverride All
         Require all granted
        </Directory>

        <Files ~ ".bak$">
         Order allow,deny
         Deny from all
        </Files>

        <Files ~ ".old$">
         Order allow,deny
         Deny from all
        </Files>

         SetEnvIf Referer “^http://localhost/”  local_referal
         SetEnvIf Referer “^http://127.0.0.1/”  local_referal
         SetEnvIf Referer “^http://file.thebom.pro/” local_referal
         SetEnvIf Referer “^$”                  local_referal

         <Directory /data/files/>
          Order Deny,Allow
          Deny  from all
          Allow from env=local_referal
        </Directory>
</VirtualHost>

