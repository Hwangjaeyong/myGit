### NEWYORK ###

<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot "/home/nychat/public_html"
        ServerName ny-wv-chat.thebom.pro
        Redirect permanent / https://ny-wv-chat.thebom.pro
        SetEnvIfNoCase Request_URI (gif|png|jpg|css|js|bmp|jpeg|swf)$ IMAGE=1
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/ny-wv-chat.thebom.pro-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/ny-wv-chat.thebom.pro-error_log.%Y%m%d"
</VirtualHost>

