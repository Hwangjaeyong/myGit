### SPS ###
<VirtualHost *:80>
        ServerAdmin webmaster@dummy-host.example.com
        DocumentRoot /home/nyapis/public_html
        ServerName dev-aws-sps.theboms.co.kr
        Redirect permanent / https://dev-aws-sps.theboms.co.kr
        SetEnvIfNoCase Request_URI (gif|png|jpg|css|js|bmp|jpeg|swf)$ IMAGE=1
        CustomLog "|/usr/sbin/cronolog logs/%Y/%m/dev-aws-sps.theboms.co.kr-access_log.%Y%m%d" combined
        ErrorLog "|/usr/sbin/cronolog logs/%Y/%m/dev-aws-sps.theboms.co.kr-error_log.%Y%m%d"

        <Directory /home/nyapis/public_html/>
         Options FollowSymLinks MultiViews
         AllowOverride All
         Require all granted
        </Directory>

        <Files ~ ".bak$">
         Order allow,deny
         Deny from all
        </Files>

        <Files ~ ".old$">
         Order allow,deny
         Deny from all
        </Files>
</VirtualHost>

